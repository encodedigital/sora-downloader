{
  "manifest_version": 3,
  "name": "encodeDigital Sora Downloader",
  "version": "1.0",
  "description": "Adds a download button below each video on a webpage.",
  "icons": {
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "permissions": []
}
